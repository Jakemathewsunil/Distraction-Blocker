{
    "manifest_version": 3,
    "name": "Distraction Blocker",
    "version": "1.0",
    "description": "Block distracting websites",
    "permissions": [
      "storage",
      "declarativeNetRequest"
    ],
    "background": {
      "service_worker": "background.js"
    },
    "action": {
      "default_popup": "popup.html"
    }
  }
  